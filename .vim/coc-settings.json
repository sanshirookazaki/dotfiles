{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "filetypes": ["go"]
        },
        "golangci-lint-languageserver": {
            "command": "golangci-lint-langserver",
            "filetypes": ["go"],
            "initializationOptions": {
                "command": ["golangci-lint", "run", "--enable-all", "--disable", "lll", "--out-format", "json"]
            }
        }
    },
    // "yaml.schemas": {
    //   "kubernetes": "/*.yaml"
    // },
    "coc.preferences.jumpCommand": "tab drop"
}
